<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/pref_general">
    <CheckBoxPreference
        android:defaultValue="false"
        android:enabled="false"
        android:key="pref_show_nsfw"
        android:title="@string/pref_show_nsfw" />

    <CheckBoxPreference
        android:defaultValue="false"
        android:enabled="false"
        android:key="pref_confirm_exit"
        android:title="@string/pref_confirm_exit" />

    <CheckBoxPreference
        android:defaultValue="true"
        android:enabled="false"
        android:key="pref_changelog"
        android:summary="@string/pref_changelog_summary"
        android:title="@string/pref_changelog" />
</PreferenceScreen>